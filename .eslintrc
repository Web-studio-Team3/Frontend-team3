{
	"extends": ["react-app", "plugin:prettier/recommended"],
	"plugins": ["prettier"],
	"rules": {
		"import/no-anonymous-default-export": "off",
		// "no-console": "error",
		"prettier/prettier": [
			"0",
			{
				"endOfLine": "auto"
			}
		],
		"import/order": [
			"off",
			{
				"groups": ["builtin", "external", "internal"],
				"pathGroups": [
					{
						"pattern": "react",
						"group": "external",
						"position": "before"
					}
				],
				"pathGroupsExcludedImportTypes": ["react"],
				"alphabetize": {
					"order": "asc",
					"caseInsensitive": true
				}
			}
		]
	}
}
